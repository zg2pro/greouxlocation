{
  "name": "greouxlocation",
  "version": "0.0.1-SNAPSHOT",
  "description": "for renting an apartment in southern France",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "clean": "rimraf target && rimraf dist",
    "build:html": "copyfiles -f src/app/index.html src/app/**/*.css src/app/**/*.js src/app/**/*.ico target/app",
    "build:resources": "ncp src/app target/app && rimraf target/app/ts",
    "compile": "npm run clean && npm run build:html && npm run build:resources && npm run build",
    "package:dependencies": "copy-node-modules . dist && replace \\.\\.\\/\\.\\.\\/node_modules node_modules dist/index.html dist/systemjs.config.js",
    "package:html": "copyfiles -f target/app/*.* dist",
    "package:templates": "copyfiles -f target/app/templates/*.* dist/templates",
    "package:ts": "copyfiles -f target/app/ts/*.* dist/ts",
    "package": "npm run compile && npm run package:html && npm run package:dependencies && npm run package:templates && npm run package:ts"
  },
  "keywords": [],
  "author": "zg2pro",
  "license": "free",
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "~0.3.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "copy-node-modules": "^1.0.2",
    "copyfiles": "^1.2.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "ncp": "^2.0.0",
    "protractor": "~4.0.14",
    "replace": "^0.3.0",
    "rimraf": "^2.6.1",
    "tslint": "^3.15.1",
    "typescript": "~2.1.0"
  },
  "repository": {}
}
